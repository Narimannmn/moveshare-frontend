  "components": {
    "schemas": {
      "AdditionalService": {
        "type": "string",
        "enum": ["packing_boxes", "bulky_items", "inventory_list", "hosting"],
        "title": "AdditionalService"
      },
      "BedroomCount": {
        "type": "string",
        "enum": ["1_bedroom", "2_bedroom", "3_bedroom", "4_bedroom", "5_bedroom", "6_plus_bedroom"],
        "title": "BedroomCount"
      },
      "Body_create_job_api_v1_jobs_post": {
        "properties": {
          "job_type": { "$ref": "#/components/schemas/JobType" },
          "description": { "type": "string", "title": "Description" },
          "pickup_address": { "type": "string", "title": "Pickup Address" },
          "delivery_address": { "type": "string", "title": "Delivery Address" },
          "pickup_datetime": {
            "type": "string",
            "format": "date-time",
            "title": "Pickup Datetime"
          },
          "delivery_datetime": {
            "type": "string",
            "format": "date-time",
            "title": "Delivery Datetime"
          },
          "payout_amount": {
            "anyOf": [
              { "type": "number" },
              { "type": "string", "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$" }
            ],
            "title": "Payout Amount"
          },
          "cut_amount": {
            "anyOf": [
              { "type": "number" },
              { "type": "string", "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$" }
            ],
            "title": "Cut Amount"
          },
          "bedroom_count": {
            "anyOf": [{ "$ref": "#/components/schemas/BedroomCount" }, { "type": "null" }]
          },
          "additional_services": {
            "type": "string",
            "title": "Additional Services",
            "default": ""
          },
          "loading_assistance_count": {
            "type": "integer",
            "title": "Loading Assistance Count",
            "default": 0
          },
          "item_images": {
            "items": { "type": "string", "format": "binary" },
            "type": "array",
            "title": "Item Images",
            "default": []
          },
          "inventory_pdf": {
            "anyOf": [{ "type": "string", "format": "binary" }, { "type": "null" }],
            "title": "Inventory Pdf"
          }
        },
        "type": "object",
        "required": [
          "job_type",
          "description",
          "pickup_address",
          "delivery_address",
          "pickup_datetime",
          "delivery_datetime",
          "payout_amount",
          "cut_amount"
        ],
        "title": "Body_create_job_api_v1_jobs_post"
      },
      "Body_upload_company_document_api_v1_company_documents_upload_post": {
        "properties": {
          "document_type": { "$ref": "#/components/schemas/DocumentType" },
          "file": { "type": "string", "format": "binary", "title": "File" }
        },
        "type": "object",
        "required": ["document_type", "file"],
        "title": "Body_upload_company_document_api_v1_company_documents_upload_post"
      },
      "CompanyProfileResponse": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" },
          "name": { "type": "string", "title": "Name" },
          "email": { "type": "string", "format": "email", "title": "Email" },
          "address": { "type": "string", "title": "Address" },
          "state": { "type": "string", "title": "State" },
          "city": { "type": "string", "title": "City" },
          "zip_code": { "type": "string", "title": "Zip Code" },
          "mc_license_number": { "type": "string", "title": "Mc License Number" },
          "dot_number": { "type": "string", "title": "Dot Number" },
          "contact_person": { "type": "string", "title": "Contact Person" },
          "phone_number": { "type": "string", "title": "Phone Number" },
          "description": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Description"
          },
          "created_at": { "type": "string", "format": "date-time", "title": "Created At" },
          "updated_at": { "type": "string", "format": "date-time", "title": "Updated At" }
        },
        "type": "object",
        "required": [
          "id",
          "name",
          "email",
          "address",
          "state",
          "city",
          "zip_code",
          "mc_license_number",
          "dot_number",
          "contact_person",
          "phone_number",
          "description",
          "created_at",
          "updated_at"
        ],
        "title": "CompanyProfileResponse"
      },
      "DocumentStatus": {
        "type": "string",
        "enum": ["pending", "approved", "rejected"],
        "title": "DocumentStatus"
      },
      "DocumentType": {
        "type": "string",
        "enum": ["mc_license", "dot_certificate", "insurance_certificate", "business_license"],
        "title": "DocumentType"
      },
      "ForgotPasswordRequest": {
        "properties": { "email": { "type": "string", "format": "email", "title": "Email" } },
        "type": "object",
        "required": ["email"],
        "title": "ForgotPasswordRequest"
      },
      "ForgotPasswordResetRequest": {
        "properties": { "password": { "type": "string", "minLength": 8, "title": "Password" } },
        "type": "object",
        "required": ["password"],
        "title": "ForgotPasswordResetRequest"
      },
      "ForgotPasswordResetResponse": {
        "properties": { "message": { "type": "string", "title": "Message" } },
        "type": "object",
        "required": ["message"],
        "title": "ForgotPasswordResetResponse"
      },
      "ForgotPasswordResponse": {
        "properties": {
          "message": { "type": "string", "title": "Message" },
          "email": { "type": "string", "format": "email", "title": "Email" }
        },
        "type": "object",
        "required": ["message", "email"],
        "title": "ForgotPasswordResponse"
      },
      "ForgotPasswordVerifyOTPRequest": {
        "properties": {
          "email": { "type": "string", "format": "email", "title": "Email" },
          "otp_code": { "type": "string", "maxLength": 6, "minLength": 6, "title": "Otp Code" }
        },
        "type": "object",
        "required": ["email", "otp_code"],
        "title": "ForgotPasswordVerifyOTPRequest"
      },
      "ForgotPasswordVerifyOTPResponse": {
        "properties": {
          "temp_token": { "type": "string", "title": "Temp Token" },
          "message": { "type": "string", "title": "Message" }
        },
        "type": "object",
        "required": ["temp_token", "message"],
        "title": "ForgotPasswordVerifyOTPResponse"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "JobResponse": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" },
          "company_id": { "type": "string", "format": "uuid", "title": "Company Id" },
          "job_type": { "$ref": "#/components/schemas/JobType" },
          "bedroom_count": {
            "anyOf": [{ "$ref": "#/components/schemas/BedroomCount" }, { "type": "null" }]
          },
          "description": { "type": "string", "title": "Description" },
          "pickup_address": { "type": "string", "title": "Pickup Address" },
          "delivery_address": { "type": "string", "title": "Delivery Address" },
          "additional_services": {
            "items": { "$ref": "#/components/schemas/AdditionalService" },
            "type": "array",
            "title": "Additional Services"
          },
          "loading_assistance_count": { "type": "integer", "title": "Loading Assistance Count" },
          "item_images": { "items": { "type": "string" }, "type": "array", "title": "Item Images" },
          "inventory_pdf": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Inventory Pdf"
          },
          "pickup_datetime": {
            "type": "string",
            "format": "date-time",
            "title": "Pickup Datetime"
          },
          "delivery_datetime": {
            "type": "string",
            "format": "date-time",
            "title": "Delivery Datetime"
          },
          "payout_amount": {
            "type": "string",
            "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
            "title": "Payout Amount"
          },
          "cut_amount": {
            "type": "string",
            "pattern": "^(?!^[-+.]*$)[+-]?0*\\d*\\.?\\d*$",
            "title": "Cut Amount"
          },
          "status": { "$ref": "#/components/schemas/JobStatus" },
          "created_at": { "type": "string", "format": "date-time", "title": "Created At" },
          "updated_at": { "type": "string", "format": "date-time", "title": "Updated At" }
        },
        "type": "object",
        "required": [
          "id",
          "company_id",
          "job_type",
          "bedroom_count",
          "description",
          "pickup_address",
          "delivery_address",
          "additional_services",
          "loading_assistance_count",
          "item_images",
          "inventory_pdf",
          "pickup_datetime",
          "delivery_datetime",
          "payout_amount",
          "cut_amount",
          "status",
          "created_at",
          "updated_at"
        ],
        "title": "JobResponse"
      },
      "JobStatus": {
        "type": "string",
        "enum": ["draft", "listed", "assigned", "in_progress", "completed", "cancelled"],
        "title": "JobStatus"
      },
      "JobType": {
        "type": "string",
        "enum": ["residential", "office", "storage"],
        "title": "JobType"
      },
      "LoginRequest": {
        "properties": {
          "email": { "type": "string", "format": "email", "title": "Email" },
          "password": { "type": "string", "title": "Password" }
        },
        "type": "object",
        "required": ["email", "password"],
        "title": "LoginRequest"
      },
      "LoginResponse": {
        "properties": {
          "access_token": { "type": "string", "title": "Access Token" },
          "refresh_token": { "type": "string", "title": "Refresh Token" },
          "user": { "$ref": "#/components/schemas/UserInfo" },
          "token_type": { "type": "string", "title": "Token Type", "default": "bearer" }
        },
        "type": "object",
        "required": ["access_token", "refresh_token", "user"],
        "title": "LoginResponse"
      },
      "LogoutAllResponse": {
        "properties": { "message": { "type": "string", "title": "Message" } },
