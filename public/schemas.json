          "address": { "type": "string", "maxLength": 500, "minLength": 5, "title": "Address" },
          "state": { "type": "string", "maxLength": 100, "minLength": 2, "title": "State" },
          "city": { "type": "string", "maxLength": 100, "minLength": 2, "title": "City" },
          "zip_code": { "type": "string", "maxLength": 20, "minLength": 5, "title": "Zip Code" },
          "mc_license_number": {
            "type": "string",
            "maxLength": 100,
            "minLength": 1,
            "title": "Mc License Number"
